{
  "name": "@appfolio-im/react-gears-cypress",
  "version": "0.0.0",
  "packageManager": "npm@9.2.0",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AppFolio-IM/react-gears-cypress"
  },
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib"
  ],
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".js,.jsx,.ts,.tsx\" --config-file ./.prod.babelrc",
    "build:bundle": "webpack --mode production",
    "clean": "rm -Rf dist lib",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix cypress/**/*.ts cypress/**/*.tsx src/**/*.ts",
    "start": "cypress open --component",
    "test": "cypress run --component",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@appfolio/react-gears": "^5.19.2",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.10.1",
    "@cypress/code-coverage": "^3.9.2",
    "@cypress/react": "^7",
    "@types/eslint": "^8.4.6",
    "@types/lodash": "^4.14.189",
    "@types/uglify-js": "^3.17.1",
    "@types/webpack-sources": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-loader": "^8.3.0",
    "cypress": "*",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.7.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsify": "^5.0.4",
    "typescript": "< 4.9.0",
    "webpack": "^4.46",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "core-js": "^3.26"
  },
  "peerDependencies": {
    "cypress": "*"
  }
}
